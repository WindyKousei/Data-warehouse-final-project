			JOIN [dbo].[film_category] [st2]
			ON [st1].[film_id] = [st2].[film_id]
			JOIN [dbo].[category] [st3]
			ON [st2].[category_id] = [st3].[category_id]
		GROUP BY
			[st1].[film_id]
	) [q]
	ON [t1].[film_id] = [q].[film_id]</property>
